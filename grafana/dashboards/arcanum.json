{
  "id": null,
  "title": "Arcanum \u2014 Minimal Token Orchestrator",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "type": "stat",
      "title": "Requests (last 1h)",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(increase(arcanum_requests_total[1h]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Error Rate % (last 1h)",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "100 * (sum(increase(arcanum_requests_total{status!=\"ok\"}[1h])) / clamp_min(sum(increase(arcanum_requests_total[1h])), 1))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Requests by Provider / Status (rate/5m)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      },
      "targets": [
        {
          "expr": "sum by (provider, status) (rate(arcanum_requests_total[5m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "bargauge",
      "title": "Success vs Errors (last 1h)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "sum by (status) (increase(arcanum_requests_total[1h]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "piechart",
      "title": "Provider Share (last 1h)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 20
      },
      "targets": [
        {
          "expr": "sum by (provider) (increase(arcanum_requests_total[1h]))",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table"
        }
      }
    }
  ],
  "templating": {
    "list": []
  },
  "timepicker": {}
}